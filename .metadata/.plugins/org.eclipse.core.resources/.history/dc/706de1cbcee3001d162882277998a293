package com.java.main;

public class AppController {
	
	//시스템을 정해주는 기능
	public void chooseSystem(int selectNumber) {
		switch (selectNumber) {
		case 1:
			
			break;
		case 2:
			
			break;
		case 3:
			
			break;
		case 4:
			System.out.println("# 프로그램을 종료합니다");
			System.exit(0);
			break;

		default:
			
			break;
		}
	}

}
