import static com.java.view.AppUI.*;

public class OrderService implements AppService {

	@Override
	public void start() {
		orderManagementScreen();
		int selection = inputInteger();
		
		switch (selection) {
		case 1:
			
			break;
		case 2:
			
			break;
		case 3:
			return;

		default:
			System.out.println("메뉴를 다시 입력하세요.");
		}
		
	}
	
}
















